<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="SelenideAutoTests" verbose="1">

    <listeners>
        <listener class-name="casino.A_BaseTestListener"/>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>

    <test name="STG77 login tests">
        <classes>
            <class name="casino.stg77.LogInTests" />
        </classes>
    </test>

    <test name="Admin login tests">
        <classes>
            <class name="casino.admin.AdminTests" />
        </classes>
    </test>

</suite>
